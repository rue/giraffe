  <li><a href="/e/<%= @page.uri %>" class="nav_link">edit</a></li>
</ul>

<div class="content">
  <pre class="sh_diff"><%= @diff %></pre>
</div>

<script type="text/javascript">
  sh_highlightDocument();
</script>
